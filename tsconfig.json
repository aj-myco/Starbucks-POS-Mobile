{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "strict": false,
    "baseUrl": ".",
    "paths": {
      "assets/*": ["assets/*"]
    }
  },
  "include": ["app", "components", ".expo/types/**/*.ts", "expo-env.d.ts"]
}